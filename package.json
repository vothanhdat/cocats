{
  "name": "game-engine",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "test": "nodemon --exec ts-node -r tsconfig-paths/register",
    "server": "nodemon --watch src/server --watch src/utilities --exec ts-node -r tsconfig-paths/register src/server/index.ts ",
    "client": "webpack-dev-server --env=dev --hot --inline --colors --watch --progress",
    "build": "webpack --env=pro --progress --profile --colors",
    "start": "npm run server & npm run client",
    "watch": "tmux   new-session  \"npm run client\" \\;   split-window \"npm run server\" \\; select-layout even-horizontal",
    "postinstall": "npm run build",
    "protogen": "ts-node -r tsconfig-paths/register src/datamodel/generator.ts --pb > src/datamodel/data.proto",
    "typegen": "ts-node -r tsconfig-paths/register src/datamodel/generator.ts --ts > src/datamodel/data.d.ts",
    "pbts1": "pbjs -t static-module -w commonjs -o src/datamodel/modal.js src/datamodel/data.proto",
    "pbts2": "pbts -o src/datamodel/modal.d.ts src/datamodel/modal.js",
    "migrate": "npm run protogen && npm run typegen && npm run pbts1 && npm run pbts2"
  },
  "author": "Vo Thanh Dat",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "0.0.33",
    "@types/engine.io": "git+https://github.com/vothanhdat/engine.io.d.ts.git",
    "@types/engine.io-client": "git+https://github.com/vothanhdat/engine.io-client.d.ts.git",
    "@types/express": "^4.0.36",
    "@types/howler": "^2.0.2",
    "@types/pixi.js": "^4.5.2",
    "@types/yargs": "^6.6.0",
    "awesome-typescript-loader": "^3.2.2",
    "babili-webpack-plugin": "^0.1.2",
    "clone": "^2.1.1",
    "compression": "^1.7.0",
    "copy-webpack-plugin": "^4.0.1",
    "deep-equal": "^1.0.1",
    "engine.io": "^3.1.0",
    "engine.io-client": "^3.1.1",
    "express": "^4.15.2",
    "express-static-gzip": "^0.2.3",
    "glob": "^7.1.1",
    "howler": "^2.0.4",
    "html-webpack-plugin": "^2.29.0",
    "image-size": "^0.5.5",
    "loader-utils": "^1.1.0",
    "pixi-extra-filters": "git+https://github.com/pixijs/pixi-extra-filters.git",
    "pixi.js": "^4.5.4",
    "protobufjs": "^6.8.0",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.3.2",
    "tsconfig-paths": "^2.1.2",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "ts-node": "^3.3.0",
    "webpack-dev-server": "^2.6.1"
  },
  "engines": {
    "node": "7.10.0",
    "npm": "4.2.0"
  }
}
