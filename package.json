{
  "name": "game-engine",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "test": "nodemon --exec ts-node -r tsconfig-paths/register",
    "server": "nodemon --watch src/server --exec ts-node -r tsconfig-paths/register src/server/index.ts ",
    "client": "webpack-dev-server --env=dev --hot --inline --colors --watch --progress",
    "build": "webpack --env=pro --progress --profile --colors",
    "start": "npm run server & npm run client",
    "watch": "tmux   new-session  \"npm run client\" \\;   split-window \"npm run server\" \\; select-layout even-horizontal",
    "postinstall": "npm run build",
    "pbts": "pbjs -t static-module -w commonjs -o src/datamodel/modal.js src/datamodel/data.proto && pbts -o src/datamodel/modal.d.ts src/datamodel/modal.js"
  },
  "author": "Vo Thanh Dat",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "0.0.33",
    "@types/engine.io": "https://github.com/vothanhdat/engine.io.d.ts",
    "@types/engine.io-client": "https://github.com/vothanhdat/engine.io-client.d.ts",
    "@types/express": "^4.0.35",
    "@types/howler": "^2.0.2",
    "@types/pixi.js": "^4.4.4",
    "awesome-typescript-loader": "^3.1.3",
    "babili-webpack-plugin": "^0.1.1",
    "clone": "^2.1.1",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "deep-equal": "^1.0.1",
    "engine.io": "^3.1.0",
    "engine.io-client": "^3.1.1",
    "express": "^4.15.2",
    "express-static-gzip": "^0.2.2",
    "glob": "^7.1.1",
    "howler": "^2.0.3",
    "html-webpack-plugin": "^2.28.0",
    "image-size": "^0.5.3",
    "pixi-extra-filters": "https://github.com/pixijs/pixi-extra-filters",
    "pixi.js": "^4.5.1",
    "protobufjs": "^6.7.3",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.0.3",
    "tsconfig-paths": "^2.1.2",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
